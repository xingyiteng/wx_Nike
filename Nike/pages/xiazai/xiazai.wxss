.box{
    position:absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    height:100%;
    z-index:0;
    width: 100%;
    overflow: hidden;
}
.box swiper{
    height: 100%;
    width: 100%;
}
.box swiper swiper-item{
    height: 100%;
    width: 100%;
}
.box swiper swiper-item image{
    width: 100%;
    height: 90%;
}
.btn{
    width: 100%;
    position: absolute;
    bottom: 80rpx;
}
.btn button{
    width: 40%;
    margin:0 auto;
    font-size: 35rpx;
    height: 80rpx;
    color: #ffffff;
    line-height: 80rpx;
    background: #111111;
}
.tk{
    position: relative;
    z-index: 10;
}