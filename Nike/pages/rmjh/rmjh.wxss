.img{
    width: 100%;
}
.img image{
    width: 100%;
    height: 400rpx;
}
.con{
    width: 100%;
    text-align: center;
    margin-top: 80rpx;
}
.con .con1 view:nth-child(1){
    font-size: 32rpx;
}
.con .con1 view:nth-child(2){
    margin-top: 10rpx;
    font-size: 28rpx;
    color: #999999;
}
.con2{
    margin-top: 80rpx;
    padding-bottom: 80rpx;
    border-bottom:1px solid #eee;
}
.con .con2 view:nth-child(1){
    font-size: 32rpx;
}
.con .con2 view:nth-child(2){
    margin-top: 10rpx;
    font-size: 28rpx;
    color: #999999;
}
.con3 navigator{
    margin: 80rpx auto;
    display: block;
    width: 80%;
    background: #000000;
    border-radius: 10rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #ffffff;
    text-align: center;
    font-size: 36rpx;
}
.con4 navigator{
    background: #999999;
}