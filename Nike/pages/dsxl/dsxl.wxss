.box navigator{
    display: block;
    padding: 30rpx;
    display: flex;
    border-bottom: 2rpx solid #999999;
}
.box image{
    width: 200rpx;
    height: 200rpx;
}
.box .box_R{
    margin-left: 30rpx;
    height: 140rpx;
    flex-grow: 0.8;
    padding-top:60rpx;
    font-size: 32rpx;
}
.box .box_R view:nth-child(2){
    color: #999999;
}
.btn{
    margin: 30rpx;
}
.btn button{
    width: 60%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 30rpx;
    color: #ffffff;
    background: #000000;
}
