.box{
    position:absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    height:100%;
    z-index:0;
    width: 100%;
    overflow: hidden;
}
.box swiper{
    height: 100%;
    width: 100%;
}
.box swiper swiper-item{
    height: 100%;
    width: 100%;
}
.box swiper swiper-item image{
    width: 100%;
    height: 1200rpx;
}
.btn{
    width: 100%;
    height: 60rpx;
    position: absolute;
    bottom:0;
    margin-bottom:100rpx;
}
.btn button{
    display: block;
    width: 45%;
    margin: 0 auto;
    line-height: 80rpx;
    height: 80rpx;
    font-size: 35rpx;
    text-align: center;
    border-radius: 10rpx;
    background: #ffffff;
}
